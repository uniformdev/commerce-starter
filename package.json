{
  "name": "commerce-starter",
  "version": "5.0.17",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s uniform:pull:manifest dev:next",
    "dev:next": "next dev",
    "build": "run-s uniform:pull:manifest build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,scss}\"",
    "uniform:pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:datatypes": "uniform canvas datatype pull ./content/datatypes",
    "uniform:pull:categories": "uniform canvas category pull ./content/categories",
    "uniform:pull:components": "uniform canvas component pull ./content/components",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/compositions",
    "uniform:pull:project-map-definitions": "uniform project-map definition pull ./content/project-map-definitions",
    "uniform:pull:project-map-nodes": "uniform project-map node pull ./content/project-map-nodes",
    "uniform:pull": "run-s uniform:pull:datatypes uniform:pull:categories uniform:pull:components uniform:pull:compositions uniform:pull:project-map-definitions uniform:pull:project-map-nodes",
    "uniform:push:datatypes": "uniform canvas datatype push ./content/datatypes",
    "uniform:push:categories": "uniform canvas category push ./content/categories",
    "uniform:push:components": "uniform canvas component push ./content/components",
    "uniform:push:compositions": "uniform canvas composition push ./content/compositions",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/project-map-definitions",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/project-map-nodes",
    "uniform:push": "run-s uniform:push:datatypes uniform:push:categories uniform:push:components uniform:push:compositions uniform:push:project-map-definitions uniform:push:project-map-nodes",
    "uniform:compositions:publish": "uniform canvas composition push ./content/compositions --state published",
    "uniform:manifest:publish": "uniform context manifest publish",
    "uniform:publish": "run-s uniform:compositions:publish uniform:manifest:publish",
    "postinstall": "cd cli-run && npm install && npm run build",
    "cli": "npm i && cd cli-run && node dist/index.mjs"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@uniformdev/canvas": "19.67.0",
    "@uniformdev/canvas-next": "19.67.0",
    "@uniformdev/canvas-react": "19.67.0",
    "@uniformdev/context": "19.67.0",
    "@uniformdev/context-devtools": "19.67.0",
    "@uniformdev/context-gtag": "19.67.0",
    "@uniformdev/context-next": "19.67.0",
    "@uniformdev/context-react": "19.67.0",
    "@uniformdev/project-map": "19.67.0",
    "@uniformdev/richtext": "19.67.0",
    "@vercel/og": "^0.5.7",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "daisyui": "^2.51.5",
    "framer-motion": "10.16.2",
    "lodash.debounce": "^4.0.8",
    "next": "13.4.19",
    "nextjs-cors": "^2.1.2",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-multi-carousel": "^2.8.3",
    "react-player": "^2.12.0",
    "react-responsive-masonry": "2.1.7",
    "sass": "1.64.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/color": "^3.0.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-responsive-masonry": "2.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/types": "^6.4.0",
    "@uniformdev/cli": "19.67.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "shared": "*",
    "tailwind-config": "*",
    "tailwindcss": "^3.3.1",
    "typescript": "5.2.2"
  }
}
