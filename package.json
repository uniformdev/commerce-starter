{
  "name": "fake-commerce",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s uniform:pull:manifest dev:next",
    "dev:next": "next dev --port 3000",
    "build": "run-s uniform:pull:manifest build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint src",
    "uniform:pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:canvas": "run-s uniform:pull:components uniform:pull:compositions",
    "uniform:pull:compositions": "uniform canvas composition pull ./data/commerce-starter.json",
    "uniform:pull:components": "uniform canvas component pull ./data/commerce-starter.json",
    "uniform:pull:context": "npm run uniform:pull:context:signals",
    "uniform:pull:context:signals": "uniform context signal pull ./data/commerce-starter.json",
    "uniform:pull:map:def": "uniform prm definition pull ./data/commerce-starter.json",
    "uniform:pull:map:nodes": "uniform prm node pull ./data/commerce-starter.json",
    "uniform:pull:map": "run-s uniform:pull:map:def uniform:pull:map:nodes",
    "uniform:push": "run-s uniform:push:context uniform:publish:context uniform:push:map uniform:push:canvas",
    "uniform:push:context": "npm run uniform:push:context:signals",
    "uniform:push:context:signals": "uniform context signal push ./data/commerce-starter.json",
    "uniform:publish:context": "uniform context manifest publish",
    "uniform:push:canvas": "run-s uniform:push:components uniform:push:compositions uniform:publish",
    "uniform:push:components": "uniform canvas component push ./data/commerce-starter.json",
    "uniform:push:compositions": "uniform canvas composition push ./data/commerce-starter.json",
    "uniform:publish": "uniform canvas composition push ./data/commerce-starter.json --state published",
    "uniform:push:map:def": "uniform prm definition push ./data/commerce-starter.json",
    "uniform:push:map:nodes": "uniform prm node push ./data/commerce-starter.json",
    "uniform:push:map": "run-s uniform:push:map:def uniform:push:map:nodes"
  },
  "dependencies": {
    "@uniformdev/canvas": "18.35.1-alpha.22",
    "@uniformdev/canvas-fake-commerce": "^17.1.1-alpha.18",
    "@uniformdev/canvas-next": "18.35.1-alpha.22",
    "@uniformdev/canvas-react": "18.35.1-alpha.22",
    "@uniformdev/cli": "18.35.1-alpha.22",
    "@uniformdev/context": "18.35.1-alpha.22",
    "@uniformdev/context-devtools": "18.35.1-alpha.22",
    "@uniformdev/context-next": "18.35.1-alpha.22",
    "@uniformdev/context-react": "18.35.1-alpha.22",
    "@uniformdev/project-map": "18.35.1-alpha.22",
    "classnames": "^2.3.2",
    "lodash.debounce": "^4.0.8",
    "next": "^13.2.4",
    "nextjs-cors": "^2.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-multi-carousel": "^2.8.2",
    "sass": "^1.57.1"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.5"
  }
}
