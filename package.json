{
  "name": "commerce-starter",
  "version": "3.1.108",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s uniform:pull:manifest dev:next",
    "dev:next": "next dev --port 3000",
    "build": "run-s uniform:pull:manifest build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "uniform:pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:datatype": "uniform canvas datatype pull ./content/baseline.json",
    "uniform:pull:components": "uniform canvas component pull ./content/baseline.json",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/baseline.json",
    "uniform:pull:project-map-definitions": "uniform project-map definition pull ./content/baseline.json",
    "uniform:pull:project-map-nodes": "uniform project-map node pull ./content/baseline.json",
    "uniform:pull:context:signals": "uniform context signal pull ./content/baseline.json",
    "uniform:pull:context:enrichments": "uniform context enrichment pull ./content/baseline.json",
    "uniform:pull:context:quirks": "uniform context quirk pull ./content/baseline.json",
    "uniform:pull:context": "run-s uniform:pull:context:signals uniform:pull:context:enrichments uniform:pull:context:quirks",
    "uniform:pull": "run-s uniform:pull:components uniform:pull:compositions uniform:pull:datatype uniform:pull:project-map-definitions uniform:pull:project-map-nodes",
    "uniform:push:datatype": "uniform canvas datatype push ./content/baseline.json",
    "uniform:push:components": "uniform canvas component push ./content/baseline.json",
    "uniform:push:compositions": "uniform canvas composition push ./content/baseline.json",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/baseline.json",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/baseline.json",
    "uniform:push:context:signals": "uniform context signal push ./content/baseline.json",
    "uniform:push:context:enrichments": "uniform context enrichment push ./content/baseline.json",
    "uniform:push:context:quirks": "uniform context quirk push ./content/baseline.json",
    "uniform:push:context:": "run-s uniform:push:context:quirks uniform:push:context:enrichments uniform:push:context:signals",
    "uniform:push": "run-s uniform:push:components uniform:push:compositions uniform:push:datatype uniform:push:project-map-definitions uniform:push:project-map-nodes",
    "uniform:composition:publish": "uniform canvas composition push ./content/baseline.json --state published",
    "uniform:manifest:publish": "uniform context manifest publish",
    "uniform:publish": "run-s uniform:composition:publish uniform:manifest:publish",
    "postinstall": "cd cli-run && npm install && npm run build",
    "cli": "cd cli-run && node dist/index.mjs"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@uniformdev/canvas": "19.54.2",
    "@uniformdev/canvas-next": "19.54.2",
    "@uniformdev/canvas-react": "19.54.2",
    "@uniformdev/context": "19.54.2",
    "@uniformdev/context-devtools": "19.54.2",
    "@uniformdev/context-gtag": "19.54.2",
    "@uniformdev/context-next": "19.54.2",
    "@uniformdev/context-react": "19.54.2",
    "@uniformdev/project-map": "19.54.2",
    "@uniformdev/richtext": "19.54.2",
    "@vercel/og": "^0.5.7",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "daisyui": "^2.51.5",
    "lodash.debounce": "^4.0.8",
    "next": "13.4.12",
    "nextjs-cors": "^2.1.2",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-multi-carousel": "^2.8.3",
    "react-player": "^2.12.0",
    "sass": "1.64.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/color": "^3.0.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@typescript-eslint/types": "^5.57.0",
    "@uniformdev/canvas-enhancers": "^18.9.1-alpha.15",
    "@uniformdev/cli": "19.54.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "shared": "*",
    "tailwind-config": "*",
    "tailwindcss": "^3.3.1",
    "typescript": "5.1.6"
  }
}
